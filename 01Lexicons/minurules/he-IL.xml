<lexicon xmlns="http://www.w3.org/2005/01/pronunciation-lexicon" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://www.w3.org/2005/01/pronunciation-lexicon http://www.w3.org/TR/2007/CR-pronunciation-lexicon-20071212/pls.xsd" alphabet="ipa" xml:lang="he-IL">
<!-- *************ACRONYMS************* -->

<!-- ACRONYM: DESCRIPTION-->
  <!--  
  <lexeme>
    <grapheme></grapheme>
    <alias></alias>
  </lexeme>
  -->
    
    
<!-- *************CRYPTIC: COMMANDS************* -->

  <!-- FA: Line number-->    
  <lexeme>
    <grapheme> FA</grapheme>
    <alias>, f a</alias>
  </lexeme>

<!-- HE: access to help pages -->
  <lexeme> 
    <grapheme>HE</grapheme>
    <alias>H E</alias>
  </lexeme>
  
<!-- PAX: Display minirules for all passengers except infants (FWR/PAX) -->
  <lexeme> 
    <grapheme>PAX</grapheme>
    <alias>P, A, X</alias>
  </lexeme>
  
<!-- *************CRYPTIC: SPECIAL CHARACTERS*************  -->
  <lexeme>
    <grapheme>/</grapheme>
    <alias>קו נטו</alias>
  </lexeme>
 
 
</lexicon>
